<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:background">@color/color_background</item>
    </style>

    <style name="TextViewStyle" parent="@android:style/Widget.DeviceDefault.TextView">
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textSize">@dimen/text_size12</item>
        <item name="android:textColor">@color/gray_900</item>
        <item name="android:textStyle">normal</item>
        <item name="android:includeFontPadding">false</item>
    </style>

</resources>
